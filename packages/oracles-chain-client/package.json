{
  "name": "@ixo/oracles-chain-client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.7.0",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@ixo/eslint-config": "workspace:*",
    "@ixo/jest-config": "workspace:*",
    "@ixo/typescript-config": "workspace:*",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.10.0",
    "@types/store": "2.0.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "tsup": "8.3.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@cosmjs/crypto": "0.32.4",
    "@cosmjs/encoding": "0.32.4",
    "@cosmjs/proto-signing": "0.32.4",
    "@cosmjs/stargate": "0.32.4",
    "@ixo/did-provider-x": "0.0.2",
    "@ixo/impactxclient-sdk": "2.3.0",
    "@ixo/logger": "workspace:*",
    "@ixo/matrixclient-sdk": "^0.2.1",
    "@veramo/core": "4.3.0",
    "@veramo/credential-ld": "4.3.0",
    "@veramo/credential-w3c": "4.3.0",
    "@veramo/data-store": "4.3.0",
    "@veramo/did-manager": "4.3.0",
    "@veramo/did-provider-key": "4.3.0",
    "@veramo/did-resolver": "4.3.0",
    "@veramo/key-manager": "4.3.0",
    "@veramo/kms-local": "4.3.0",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "axios": "1.7.8",
    "axios-retry": "4.5.0",
    "bs58": "^6.0.0",
    "did-resolver": "4.1.0",
    "dotenv": "^16.4.5",
    "graphql": "^16.10.0",
    "graphql-request": "7.1.2",
    "graphql-tag": "^2.12.6",
    "limiter": "2.1.0",
    "store": "2.0.12",
    "zod": "^3.23.8"
  }
}

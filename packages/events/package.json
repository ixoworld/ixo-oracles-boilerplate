{
  "name": "@ixo/oracles-events",
  "version": "1.0.1",
  "description": "Events for the IXO Oracles",
  "private": false,
  "main": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "module": "./dist/server.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "exports": {
    ".": {
      "require": "./dist/server.js",
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./server": {
      "require": "./dist/server.js",
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/types/index.js",
      "import": "./dist/types/index.js"
    }
  },
  "devDependencies": {
    "@ixo/eslint-config": "workspace:*",
    "@ixo/jest-config": "workspace:*",
    "@ixo/typescript-config": "workspace:*",
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "socket.io": "4.8.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "eventemitter2": "6.4.9"
  }
}

{
  "name": "@ixo/oracles-client-sdk",
  "version": "1.1.2",
  "type": "module",
  "private": false,
  "description": "Client SDK for interacting with QiForge oracles, designed for React applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./live-agent": {
      "types": "./dist/live-agent/index.d.ts",
      "import": "./dist/live-agent/index.js",
      "require": "./dist/live-agent/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/live-agent/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/live-agent/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@ixo/oracles-chain-client": "workspace:*",
    "@ixo/oracles-events": "workspace:*",
    "@tanstack/react-query": "^5.29.0",
    "eventsource": "^3.0.6",
    "jose": "^6.1.0",
    "livekit-client": "^2.15.6",
    "socket.io-client": "^4.8.1",
    "zod-to-json-schema": "^3.24.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ixo/eslint-config": "workspace:*",
    "@ixo/matrix": "workspace:*",
    "@ixo/typescript-config": "workspace:*",
    "@ixo/vitest-config": "workspace:*",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.2.79",
    "matrix-js-sdk": "^37.11.0",
    "react": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
